<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <connectionStrings>
    <add name="YourConnectionStringName" connectionString="YourConnectionString" />
  </connectionStrings>
  <appSettings>
    <add key="SmtpServer" value="your_smtp_server_address" />
    <add key="SmtpPort" value="587" />
    <add key="SmtpUsername" value="your_email_username" />
    <add key="SmtpPassword" value="your_email_password" />
    <add key="Pop3Server" value="your_pop3_server_address" />
    <add key="Pop3Port" value="995" />
    <add key="Pop3Username" value="your_email_username" />
    <add key="Pop3Password" value="your_email_password" />
    <add key="FromEmailAddress" value="yourEmail@example.com" />
  </appSettings>

</configuration>